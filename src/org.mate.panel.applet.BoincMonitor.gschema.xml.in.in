<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="@GETTEXT_PACKAGE@">
   <schema id="org.mate.panel.applet.BoincMonitor">
     <key type="i" name="tasks-to-display">
      <default>1</default>
      <_summary>Number of tasks to show in the panel</_summary>
      <_description>Number of tasks whose completion status is displayed in the panel. Can be between 0 (no tasks are displayed) and 8.</_description>
    </key>
    <key type="i" name="task-update-interval">
      <default>1</default>
      <_summary>How often (in minutes) task details are updated</_summary>
      <_description>How often (in minutes) the task details in the panel are updated. Can be between 1 and 60 minutes.</_description>
    </key>
   <key type="i" name="sundries-update-interval">
      <default>1</default>
      <_summary>How often (in hours) user credit details and BOINC notices are updated</_summary>
      <_description>How often (in hours) user credit totals and BOINC notices are updated. Can be between 1 and 8 hours.</_description>
    </key>
   <key type="b" name="show-total-uc">
      <default>false</default>
      <_summary>Whether the number of the user's total BOINC credits are displayed in the panel</_summary>
      <_description></_description>
    </key>
    <key type="b" name="show-today-uc">
      <default>false</default>
      <_summary>Whether the number of BOINC credits earned today are displayed in the panel</_summary>
      <_description></_description>
    </key>
    <key type="s" name="panel-graphic">
     <default>'@LOCATION@/boinc.png'</default>
     <_summary>The image to display in the panel next to Boinc tasks</_summary>
     <_description/>
    </key>	
   </schema>
</schemalist>
